> filtered_onemap <- load("/cromwell-executions/SimulatedReads/f68d4b15-5694-4a8f-ad24-54174031bc78/call-ReadSimulations/shard-0/reads_simu/fb85cb16-7561-4ee9-a847-d1e9b9005f22/call-DefaultMaps/shard-0/DefaultMaps/8f7876d3-e3e2-4770-8052-6d9dc2c355df/call-MapsReport/shard-0/inputs/1125635622/onemap_obj_filtered.RData")
> filtered_onemap <- get(filtered_onemap)
> 
> simu_onemap_obj <- load("/cromwell-executions/SimulatedReads/f68d4b15-5694-4a8f-ad24-54174031bc78/call-ReadSimulations/shard-0/reads_simu/fb85cb16-7561-4ee9-a847-d1e9b9005f22/call-DefaultMaps/shard-0/DefaultMaps/8f7876d3-e3e2-4770-8052-6d9dc2c355df/call-MapsReport/shard-0/inputs/516657223/simu_vcf_onemap.obj.RData")
> simu_onemap_obj <- get(simu_onemap_obj)
> ref_alt_alleles <- read.table("/cromwell-executions/SimulatedReads/f68d4b15-5694-4a8f-ad24-54174031bc78/call-ReadSimulations/shard-0/reads_simu/fb85cb16-7561-4ee9-a847-d1e9b9005f22/call-DefaultMaps/shard-0/DefaultMaps/8f7876d3-e3e2-4770-8052-6d9dc2c355df/call-MapsReport/shard-0/inputs/1560185715/ref_alt_alleles.txt")
> simulated_phases <- read.table("/cromwell-executions/SimulatedReads/f68d4b15-5694-4a8f-ad24-54174031bc78/call-ReadSimulations/shard-0/reads_simu/fb85cb16-7561-4ee9-a847-d1e9b9005f22/call-DefaultMaps/shard-0/DefaultMaps/8f7876d3-e3e2-4770-8052-6d9dc2c355df/call-MapsReport/shard-0/inputs/1560185715/simulated_phases.txt")
> 
> ## Without false SNPs
> times <-system.time(create_maps_report(input.seq = filtered_onemap,
+                                        tot_mks = ref_alt_alleles, gab = simu_onemap_obj,
+                                        "gatk" , "default",
+                                        fake= F, "vcf", simulated_phases))
> 
> outname <- paste0("map_", "gatk", "_", "vcf",
+                   "_", "default", "_", FALSE)
> 
> times <- data.frame(meth = outname, time = times[3])
> 
> 
> ## With false SNPs
> times_temp <-system.time(create_maps_report(input.seq = filtered_onemap,
+                                        tot_mks = ref_alt_alleles, gab = simu_onemap_obj,
+                                        "gatk" , "default",
+                                        fake= T, "vcf", simulated_phases))
> 
> outname <- paste0("map_", "gatk", "_", "vcf",
+                   "_", "default", "_", TRUE)
> 
> # Joint maps data.frames
> map_temp <- read.table("map_gatk_vcf_default_TRUE.txt")
> map_joint <- map_temp
> map_temp <- read.table("map_gatk_vcf_default_FALSE.txt")
> map_joint <- rbind(map_joint, map_temp)
> write.table(map_joint, file = "map_gatk_vcf_default.txt", col.names = F)
> 
> # Joint RDatas
> RDatas_joint <- list()
> map_temp <- load("map_gatk_vcf_default_TRUE.RData")
> RDatas_joint[[1]] <- get(map_temp)
> map_temp <- load("map_gatk_vcf_default_FALSE.RData")
> RDatas_joint[[2]] <- get(map_temp)
> names(RDatas_joint) <- c("map_gatk_vcf_default_TRUE", "map_gatk_vcf_default_FALSE")
> save(RDatas_joint, file= "map_gatk_vcf_default.RData")
> 
> # Joint times data.frames
> times_temp <- data.frame(meth = outname, time = times_temp[3])
> times <- rbind(times, times_temp)
> write.table(times, "times_gatk_vcf_default.txt", col.names = F)
